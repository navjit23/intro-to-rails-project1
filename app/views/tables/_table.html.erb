<div id="<%= dom_id table %>">
  <p>
    <strong>Name:</strong>
    <%= table.name %>
  </p>

  <p>
    <strong>Founded in:</strong>
    <%= table.founded %>
  </p>


  <p>
    <strong>Logo:</strong>
    <img src="<%= table.logo  %>" alt="logo">
  </p>

  <p>
    <strong>League:</strong>
    <%= table.league_id %>
  </p>

</div>
