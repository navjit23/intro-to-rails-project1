<p style="color: green"><%= notice %></p>

<h1>Leagues</h1>

<div id="leagues">
  <% @leagues.each do |league| %>
    <%= render league %>
    <p>
      <%= link_to "Show this league", league %>
        <h2><%= link_to league.league_name, league_path(league) %></h2>

    </p>
  <% end %>
</div>

<%= link_to "New league", new_league_path %>
