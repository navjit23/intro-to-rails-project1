<div id="<%= dom_id player %>">
  <p>
    <strong>Name:</strong>
    <%= player.name %>
  </p>

  <p>
    <strong>Age:</strong>
    <%= player.age %>
  </p>

  <p>
    <strong>Photo:</strong>
    <img src="<%= player.photo %>" alt="photo">
  </p>

  <p>
    <strong>Nationality:</strong>
    <%= player.nationality %>
  </p>

</div>
